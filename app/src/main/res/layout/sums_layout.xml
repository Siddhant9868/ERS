        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_month"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:layout_marginBottom="0dp"
        android:text="..." />

    <TextView
        android:id="@+id/todays_looters"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/todays_"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="0dp"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_sems_pitstop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_sems"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_mons_pitstop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_month"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:layout_marginBottom="0dp"
        android:text="..." />

    <TextView
        android:id="@+id/todays_pitsop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/todays_"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="0dp"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_sems_301f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_sems"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_mons_301f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_month"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:layout_marginBottom="0dp"
        android:text="..." />

    <TextView
        android:id="@+id/todays_301f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/todays_"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="0dp"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_sems_foodking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_sems"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_mons_foodking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_month"
        android:layout_centerHorizontal="true"
        android:textSize="17sp"
        android:layout_marginBottom="0dp"
        android:text="..." />

    <TextView
        android:id="@+id/todays_foodking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/todays_"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="0dp"
        android:textSize="17sp"
        android:text="..." />

    <TextView
        android:id="@+id/this_sems_story"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0277bd"
        android:layout_above="@+id/this_sems"
